CLANG_MODULES_AUTOLINK = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/GPUImage" "${PODS_ROOT}/Headers/MBProgressHUD"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/GPUImage" -isystem "${PODS_ROOT}/Headers/MBProgressHUD"
OTHER_LDFLAGS = -ObjC -framework AVFoundation -framework CoreGraphics -framework CoreMedia -framework OpenGLES -framework QuartzCore
PODS_ROOT = ${SRCROOT}/Pods